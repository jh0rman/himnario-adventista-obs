{
  "name": "himnario-adventista-broadcast",
  "version": "2.5.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test 'vite dev' http://localhost:5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev' http://localhost:5173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "obs-websocket-js": "^5.0.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.4.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.6.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  }
}
